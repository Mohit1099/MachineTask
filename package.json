{
  "name": "ts-bp",
  "version": "0.1.0",
  "description": "backend ",
  "main": "dist/server.js",
  "_moduleAliases": {
    "@api": "dist/api",
    "@cache": "dist/cache",
    "@config": "dist/config",
    "@core": "dist/core",
    "@database": "dist/database",
    "@middleware": "dist/middleware",
    "@service": "dist/service",
    "@type": "dist/type",
    "@util": "dist/util"
  },
  "scripts": {
    "lint:fix": "eslint . --ext .ts --fix",
    "lint": "eslint . --ext .ts",
    "start": "npm run serve",
    "build": "npm run lint && npm run build-ts",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --detectOpenHandles --forceExit --verbose --runInBand --coverage",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "prettier": "prettier --config --ignore-path .gitignore .prettierrc './**.ts' --check",
    "prettier:fix": "prettier --config --ignore-path .gitignore .prettierrc './**.ts' --write",
    "seed": "node dist/database/seeds/seed.js"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.9",
    "@types/hapi__joi": "^17.1.6",
    "@types/http-errors": "^1.8.0",
    "@types/jsonwebtoken": "^8.5.0",
    "axios": "^1.6.3",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "http-errors": "^1.8.0",
    "i18n": "^0.13.4",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.2",
    "mongoose": "^7.4.2",
    "swagger-jsdoc": "^6.0.6",
    "swagger-ui-express": "^4.1.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.9",
    "@types/i18n": "^0.12.0",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^14.18.42",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.16.0",
    "nodemon": "^3.0.1",
    "prettier": "^2.7.1",
    "typescript": "^4.1.3"
  }
}
